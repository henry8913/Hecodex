    "custom_liquid_qxz74i": {
      "type": "custom-liquid",
      "name": "t:sections.custom-liquid.presets.name",
      "settings": {
        "page_full_width": false,
        "page_full_width_spacing": false,
        "title": "Approfitta dello sconto del 20%.",
        "heading_size": "h2",
        "sub_heading": "üî• Sconto attivo ancora per poco.",
        "description": "L‚Äôofferta √® attiva ancora per poco. Ottieni subito il tuo Kit Hecodex e crea il tuo store Shopify in pochi minuti.",
        "button_label": "",
        "button_link": "#",
        "button_style_secondary": false,
        "column_alignment": "center",
        "custom_liquid": "{%- assign uid = id | default: 'countdown' -%}\n{%- assign hours_left = hours | default: 6 -%}\n\n<div id=\"cd-wrap-{{ uid }}\" \n     style=\"text-align:center; color:#CCFF00; font-size:2.2rem; font-weight:600; margin: 10px 0;\">\n  ‚è≥ Offerta valida ancora per <span id=\"cd-{{ uid }}\" aria-live=\"polite\">calcolo‚Ä¶</span>\n</div>\n\n<script>\n(function() {\n  var key = \"hecodex-countdown-end\";\n  var outEl = document.getElementById(\"cd-{{ uid }}\");\n  if (!outEl) return;\n\n  // Durata countdown (in ore)\n  var duration = {{ hours_left | times: 3600000 }};\n\n  // Controlla se esiste una data di scadenza salvata\n  var savedEnd = localStorage.getItem(key);\n  var endTime = savedEnd ? parseInt(savedEnd) : Date.now() + duration;\n\n  // Se non esiste, la salva\n  if (!savedEnd) localStorage.setItem(key, endTime);\n\n  function tick() {\n    var dist = endTime - Date.now();\n\n    if (dist <= 0) {\n      outEl.textContent = \"Offerta scaduta üîí\";\n      return;\n    }\n\n    var h = Math.floor(dist / 3600000);\n    var m = Math.floor((dist % 3600000) / 60000);\n    var s = Math.floor((dist % 60000) / 1000);\n    outEl.textContent = h + \"h \" + m + \"m \" + s + \"s\";\n  }\n\n  tick();\n  setInterval(tick, 1000);\n})();\n</script>",
        "color_scheme": "scheme-1",
        "padding_top": 40,
        "padding_bottom": 16
      }
    },
